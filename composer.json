{
  "name": "contributte/http",
  "description": "Extra contrib to nette/http",
  "keywords": [
    "nette",
    "http",
    "request",
    "response",
    "curl",
    "auth"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/http",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": "^7.2",
    "nette/di": "^3.0",
    "nette/http": "^3.0",
    "tracy/tracy": "^2.6"
  },
  "require-dev": {
    "ninjify/qa": "^0.9.0",
    "ninjify/nunjuck": "^0.2.0",
    "phpstan/phpstan-shim": "^0.11.5",
    "phpstan/phpstan-deprecation-rules": "^0.11.0",
    "phpstan/phpstan-nette": "^0.11.0",
    "phpstan/phpstan-strict-rules": "^0.11.0"
  },
  "suggest": {
    "nette/di": "to use CompilerExtensions"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Http\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-next": "0.4.x-dev"
    }
  }
}
